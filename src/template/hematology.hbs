<style>
  body, html { margin: 0; padding: 0; box-sizing: border-box; font-family:
  'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode',
  Geneva, Verdana, sans-serif;} .container { width: 100%; padding: 10px; }
  .header { text-align: center;} .header span { color: rgb(218, 6, 6)} table
  {width: 97%; border-collapse: collapse; text-transform: uppercase;} table tr
  th { border: 1px solid #000; text-align: left; padding: 5px 8px; font-size:
  12px; font-weight: normal;} .sub-title { text-align: center; height: 20px;
  width: 97%; background-color: red;}
</style>

<html>
  <body>
    <div class="container">
      <div class="header">
        <h2> STAT<span>LAB</span> DIAGNOSTIC LABORATORY</h2>
        <h5>Lor lin Bldg. 172-D Corner Magsaysay St. and Lizares St. Zone 6.
          Talisay City, Neg. Occ.. Phil. 6115
        </h5>
        <h5>PHONE NOS. 09568255620 / 09128677606 GMAIL: statlabtalisay@gmail.com
          FB PAGE: STAT LAB
        </h5>
      </div>
      <div class="content-detail">
        <table>
          <thead>
            <tr>
              <th style="width: 150px;">PATIENT NAME: </th>
              <th style="width: 300px;">
                {{data.patient.last_name}}
                {{data.patient.first_name}},
                {{data.patient.middle_name}}</th>
              <th style="width: 70px;">SEX: </th>
              <th>{{data.patient.sex}}</th>
            </tr>
            <tr>
              <th style="width: 150px;">ADDRESS: </th>
              <th style="width: 300px;">
                {{data.patient.address}}</th>
              <th style="width: 70px;">Date: </th>
              <th>{{dateFormat data.createdAt "LLL"}}</th>
            </tr>
            <tr>
              <th style="width: 150px;">PHYSICIAN: </th>
              <th style="width: 300px;">
                {{data.physician}}</th>
              <th style="width: 70px;">LAB NO: </th>
              <th>{{data.lab_no}}</th>
            </tr>
          </thead>
        </table>
      </div>
      <div class="sub-title">
        <h4>HEMATOLOGY</h4>
      </div>
      <div class="content-test-result" style="margin-top: 20px;">
        <table>
          <thead>
            <tr>
              <th style="width: 150px;">TEST REQUESTED</th>
              <th colspan="3" style="text-align: center;"> CBC PLATELET</th>
            </tr>
            <tr>
              <th style="width: 150px;"></th>
              <th style="width: 300px;">
                RESULT</th>
              <th style="width: 70px;">UNIT </th>
              <th>NORMAL VALUES</th>
            </tr>
            <tr>
              <th style="width: 150px;" rowspan="2">HEMOGLOBIN </th>
              <th style="width: 300px;" rowspan="2">
                {{data.hemoglobin}}
              </th>
              <th style="width: 70px;" rowspan="2"> g / L</th>
              <th rowspan="1">M 130-180</th>
            </tr>
            <tr>
              <th rowspan="1">F 120-160</th>
            </tr>

            <tr>
              <th style="width: 150px;" rowspan="2">HEMATOCRIT </th>
              <th style="width: 300px;" rowspan="2">
                {{data.hematocrit}}
              </th>
              <th style="width: 70px;" rowspan="2"> L / L</th>
              <th rowspan="1">M 0.40-0.54</th>
            </tr>
            <tr>
              <th rowspan="1">F 0.37-0.47</th>
            </tr>

            <tr>
              <th style="width: 150px;" rowspan="2">RBC COUNT </th>
              <th style="width: 300px;" rowspan="2">
                {{data.rbc_count}}
              </th>
              <th style="width: 70px;" rowspan="2"> x10 <sup>12</sup> / L</th>
              <th rowspan="1">M 4.5-6.2</th>
            </tr>
            <tr>
              <th rowspan="1">F 4.2-5.5</th>
            </tr>

            <tr>
              <th style="width: 150px;">WBC COUNT </th>
              <th style="width: 300px;">
                {{data.wbc_count}}</th>
              <th style="width: 70px;">x10 <sup>9</sup> / L</th>
              <th>4.5-10.0</th>
            </tr>
            <tr>
              <th style="width: 150px;">PLATELET COUNT </th>
              <th style="width: 300px;">
                {{data.platelet_count}}</th>
              <th style="width: 70px;">x10 <sup>9</sup> / L</th>
              <th>150-450</th>
            </tr>
          </thead>
        </table>
      </div>
      <div>
        <h5>DIFFERENTIAL COUNT</h5>
      </div>
      <div>
        <table>
          <thead>
            <tr>
              <th style="width: 150px;">NEUTROPHIL </th>
              <th style="width: 300px;">
                {{data.neutrophil}}
              </th>
              <th style="width: 70px;"> %</th>
              <th>55-65</th>
            </tr>

            <tr>
              <th style="width: 150px;">SEGMENTED </th>
              <th style="width: 300px;">
                {{data.segmented}}
              </th>
              <th style="width: 70px;"> %</th>
              <th>50-60</th>
            </tr>

            <tr>
              <th style="width: 150px;">STAB </th>
              <th style="width: 300px;">
                {{data.stab}}
              </th>
              <th style="width: 70px;"> %</th>
              <th>0-5</th>
            </tr>

            <tr>
              <th style="width: 150px;">LYMPHOCYTIES </th>
              <th style="width: 300px;">
                {{data.lymphocyties}}</th>
              <th style="width: 70px;">%</th>
              <th>25-35</th>
            </tr>
            <tr>
              <th style="width: 150px;">MONOCYTIES </th>
              <th style="width: 300px;">
                {{data.monocyties}}</th>
              <th style="width: 70px;">%</th>
              <th>4-8</th>
            </tr>
            <tr>
              <th style="width: 150px;">EOSINOPHILS </th>
              <th style="width: 300px;">
                {{data.eosinophils}}</th>
              <th style="width: 70px;">%</th>
              <th>1-3</th>
            </tr>
            <tr>
              <th style="width: 150px;">BASOPHILS </th>
              <th style="width: 300px;">
                {{data.basophils}}</th>
              <th style="width: 70px;">%</th>
              <th>0-1</th>
            </tr>
            <tr>
              <th style="width: 150px;">TOTAL </th>
              <th style="width: 300px;">
                {{total data}}</th>
              <th style="width: 70px;">%</th>
              <th></th>
            </tr>
            <tr>
              <th style="width: 150px;" rowspan="2">REMARKS </th>
              <th colspan="3" style="text-align: center;" rowspan="1">BLOOD
                TYPING</th>
            </tr>
            <tr>
              <th colspan="3" style="text-align: center;"> {{data.remarks}}</th>
            </tr>
          </thead>
        </table>
      </div>
    </div>
  </body>
</html>